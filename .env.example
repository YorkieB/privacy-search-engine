# Copy this file to .env and replace the placeholder values with your actual configuration

# Brave Search API Key (required)
BRAVE_API_KEY=your_brave_api_key_here

# Server configuration
PORT=3001
NODE_ENV=development

# CORS configuration
ORIGIN=http://localhost:3000

# News refresh settings
DEFAULT_NEWS_REFRESH_INTERVAL=300000

# Cache settings
CACHE_TTL_SECONDS=300
MAX_CACHE_ENTRIES=100

# Sentiment analysis performance threshold
SENTIMENT_TIMEOUT_MS=500